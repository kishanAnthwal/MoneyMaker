{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "client/build/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",  
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",  
      "dest": "/client/build/index.html"
    },
    {
        "src":"/about",
        "dest":"/"
      },
      {
        "src":"/blog",
        "dest":"/"
      },
      {
        "src":"/contact",
        "dest":"/"
      },
      {
       "src":"/cricketbetting",
        "dest":"/"
      },
      {
        "src":"/platform",
        "dest":"/"
      },
      {
        "src": "/articles/:id",
        "dest":"/"
      },
      {
        "src":"/batting",
        "dest":"/"
      },
      {
        "src":"/login",
       "dest":"/"
      },
      {
        "src":"/payment",
        "dest":"/"
      }
  ]
}

